{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "https://isaac-mineo-backend.onrender.com",
    "VITE_API_BASE_URL": "https://isaac-mineo-backend.onrender.com/api"
  }
}
